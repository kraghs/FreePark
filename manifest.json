{
  "manifest_version": 3,
  "name": "Parking Spots DK",
  "version": "1.0",
  "description": "Find parkeringspladser i Danmark med kort og info.",
  "permissions": ["geolocation"],
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ]
}
